<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>

    <bean class="com.mycompany.invoice.service.InvoiceServicePrefix">
        <property name="lastNumber" value="112"/>
        <property name="prefix" value="INV_"/>
    </bean>

    <bean class="com.mycompany.invoice.controller.InvoiceControllerWeb"/>

    <bean class="com.mycompany.invoice.repository.InvoiceRepositoryDatabase"/>
    

</beans>